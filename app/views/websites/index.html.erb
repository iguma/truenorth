<h1>Listing Websites</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Subtitle</th>
    <th colspan="3">Actions</th>
    </tr>

  <% @websites.each do |website| %>
      <tr>
        <td><%= website.name %></td>
        <td><%= website.title %></td>
        <td><%= website.subtitle %></td>
        <td><%= link_to 'Show', website %></td>
        <td><%= link_to 'Edit', edit_website_path(website) %></td>
        <td><%= link_to 'Destroy', website, :confirm => 'Are you sure?',
                        :method => :delete %></td>
      </tr>
  <% end %>
</table>

<div id="nav">
    <%= link_to 'New Website', new_website_path %>
</div>
